    "f752c25e-ff65-466a-ba61-33848f1147fc": {
      "type": "text--image-with-text",
      "settings": {
        "colors": "white",
        "image-width": "extra-large",
        "image": "shopify:\/\/shop_images\/Interior_Designers_Owners-Hotels_1.png",
        "layout": "left",
        "subheading": "Meet the host - Sami lamaa",
        "location": false,
        "title": "\"At Chetzeron we combine the warm and friendly atmosphere of a mountain hut in an industrial wasteland, with refined and timeless design.\"",
        "text_block": "<p><br\/><\/p>",
        "page_description": false,
        "button_link": "",
        "hotel_page_url": false,
        "button_label": "Learn More"
      }
