    "f752c25e-ff65-466a-ba61-33848f1147fc": {
      "type": "text--image-with-text",
      "settings": {
        "colors": "white",
        "image-width": "large",
        "image": "shopify:\/\/shop_images\/006-carlos-couturier-moises-micha-b.jpg",
        "layout": "left",
        "subheading": "Meet the hosts",
        "location": false,
        "title": "Carlos Couturier & Moisés Micha, Owners Grupo Habita",
        "text_block": "<p><br\/>Known for creating Mexico’s trendiest boutique properties, the hotel developer Grupo Habita are a pioneer in this space.<\/p><p><\/p>",
