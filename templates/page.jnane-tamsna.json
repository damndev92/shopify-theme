    "f752c25e-ff65-466a-ba61-33848f1147fc": {
      "type": "text--image-with-text",
      "settings": {
        "colors": "white",
        "image-width": "large",
        "image": "shopify:\/\/shop_images\/jnane_2add27f4-df31-47b6-ab10-0ec2f0042313.png",
        "layout": "left",
        "subheading": "Meet the host - Meyranne Loum-Martin",
        "location": false,
        "title": "“Marrakech has given me the opportunity to develop my creativity with incredible freedom. Here, everything is possible; you draw sketches in the morning, and the next day the ironworker comes up with the model.\"",
        "text_block": "",
