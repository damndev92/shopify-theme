    "b195b8ee-f0b3-4c9c-b196-88e03135f2b9": {
      "type": "text--image-with-text",
      "settings": {
        "colors": "white",
        "image-width": "large",
        "image": "shopify:\/\/shop_images\/pigalle.png",
