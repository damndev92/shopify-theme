    "f7c0748c-6ba1-46ec-850f-c0f1e93b3446": {
      "type": "text--rich-text",
      "settings": {
        "colors": "white",
        "width": "medium",
        "subheading": "Stockholm, Sweden",
        "page_title": "",
        "title": "Story Hotel",
        "text": "<p><br\/>The Story Hotel is an urban haven where stunningly coiffed Stockholmers and trendy travellers converge normally over cocktails inside the bustling lobby bar which has been inspired by the interiors of Parisian salons and dark, moody jazz bars.<\/p>",
        "button_link": "",
        "button_label": ""
      }
    },
    "8fbd4d37-f5a9-4f6d-ac8a-aaa42a79403d": {
      "type": "image--image",
      "settings": {
