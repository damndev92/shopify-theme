    "f752c25e-ff65-466a-ba61-33848f1147fc": {
      "type": "text--image-with-text",
      "settings": {
        "colors": "white",
        "image-width": "medium",
        "image": "shopify:\/\/shop_images\/Fields-in-Fields-Thyme-Caryn-Hibbert-DSC5404.jpg",
        "layout": "left",
        "subheading": "Meet",
        "location": false,
        "title": "Caryn Hibbert, Owner",
        "text_block": "<p><br\/>Caryn Hibbert of Thyme became interested in hospitality during her time fundraising for a children’s hospice in Surrey whilst she was still a doctor. She discovered a world outside of hospitals and medicine, and was able to bring together some of my other interests when we put on events for the hospice. She became involved in the food and wine sector and was able to learn about other things, too, like fashion and art.<\/p>",
        "page_description": false,
        "button_link": "",
        "hotel_page_url": false,
        "button_label": "Learn More"
      }
    },
