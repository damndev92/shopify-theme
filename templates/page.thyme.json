    "f7c0748c-6ba1-46ec-850f-c0f1e93b3446": {
      "type": "text--rich-text",
      "settings": {
        "colors": "white",
        "width": "medium",
        "subheading": "The Cotswolds, UK",
        "page_title": "",
        "title": "Thyme",
        "text": "<p>Thyme is a restored historic Cotswold manor and farm. It is a family story where a passion for the land, food, and entertaining, merges with a love of local heritage, beauty, and conservation.<\/p>",
        "button_link": "",
        "button_label": ""
      }
    },
    "fddaf64f-ef02-4124-a6da-30225eb3da6f": {
      "type": "image--image",
      "settings": {
        "image": "shopify:\/\/shop_images\/Untitled_design_c55be5cd-ab52-420d-92f3-960d588c3741.png",
