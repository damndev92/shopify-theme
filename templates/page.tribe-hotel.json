    "f7c0748c-6ba1-46ec-850f-c0f1e93b3446": {
      "type": "text--rich-text",
      "settings": {
        "colors": "white",
        "width": "medium",
        "subheading": "Nairobi, Kenya",
        "page_title": "",
        "title": "Tribe Hotel",
        "text": "<p><br\/>Tribe Hotel, Nairobi’s coolest luxury boutique hotel, encapsulates the city’s African spirit and celebrates its creativity. Warm, earthy shades and traditional craft blends with contemporary design, creating a sense of understated luxury. We are incredibly proud to showcase Tribe hotel’s unique decor.<\/p>",
        "button_link": "",
        "button_label": ""
      }
    },
    "84d38e23-4d01-4e43-9b45-77682a5c89f5": {
      "type": "image--image",
